{
    "version": 1,
    "formatters": {
        "default": {
            "()": "uvicorn.logging.DefaultFormatter",
            "fmt": "%(levelprefix)s %(client_addr)s - %(user_agent)s '%(method)s %(full_path)s' %(status_code)s",
            "use_colors": null
        }
    },
    "handlers": {
        "default": {
            "formatter": "default",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "uvicorn": {
            "handlers": ["default"],
            "level": "INFO"
        },
        "uvicorn.error": {
            "level": "INFO",
            "handlers": ["default"],
            "propagate": false
        },
        "uvicorn.access": {
            "handlers": ["default"],
            "level": "INFO",
            "propagate": false
        }
    }
}
